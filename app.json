{
  "expo": {
    "name": "troskyApp",
    "slug": "troskyApp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/trosky_logo1.png",

    "android": {
      "package": "com.trosky.troskyCustomerApp"
    },
    "splash": {
      "image": "./assets/images/trosky_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [ 
      "assets/images/*"
     
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trosky.troskyCustomerApp"
    }
  }
}
